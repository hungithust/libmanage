<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_RIGHT" prefHeight="617.0" prefWidth="777.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="st.librarymanagement.Controller.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="530.0" prefWidth="737.0">
        <TextField fx:id="user_name" layoutX="265.0" layoutY="137.0" prefHeight="25.0" prefWidth="208.0"
                   promptText="User Name"/>
        <PasswordField fx:id="user_password" layoutX="265.0" layoutY="184.0" prefHeight="25.0" prefWidth="208.0"
                       promptText="Pass Word"/>
        <Button fx:id="loginButton" layoutX="343.0" layoutY="237.0" mnemonicParsing="false"
                onAction="#buttonLoginActionPerformed" prefHeight="25.0" prefWidth="58.0" text="Login"/>
        <Button fx:id="signUpButton" layoutX="343.0" layoutY="288.0" mnemonicParsing="false" text="Sign Up"/>
    </AnchorPane>
</VBox>
